.product-item-2:first-child {
  margin-top: 20rpx;
}
.product-item-2 {
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 0 20rpx;
  margin-bottom: 25rpx;
}
.product-item-2 .product-image-2 {
  width: 194rpx;
  height: 194rpx;
  display: block;
  margin-right: 20rpx;
}
.uni-scroll-view::-webkit-scrollbar,
.catescroll2-top::-webkit-scrollbar {
  /*滚动条整体样式*/
  /*高宽分别对应横竖滚动条的尺寸*/
  display: block;
  width: 0rpx !important;
  height: 0rpx !important;
  -webkit-appearance: default-button;
}
.uni-scroll-view::-webkit-scrollbar-thumb,
.catescroll2-top::-webkit-scrollbar-thumb {
  /*滚动条里面小方块*/
  border-radius: 10px;
  background-color: #909399;
}
.uni-scroll-view::-webkit-scrollbar-track,
.catescroll2-top::-webkit-scrollbar-track {
  /*滚动条里面轨道*/
  box-shadow: inset 0 0 5px rgba(0, 0, 0, 0.2);
  background: #ededed;
  border-radius: 10px;
}
.price-wrap {
  position: relative;
  width: 100%;
}
.add-shopping-wrap {
  position: absolute;
  right: 0;
  top: 0;
  width: 48rpx;
  height: 48rpx;
  line-height: 48rpx;
  border-radius: 50%;
}
.add-shopping-wrap .icon {
  margin-left: 6rpx;
  color: #fff;
}
#emptyShopping {
  width: 100%;
  height: 96rpx;
  opacity: 0;
}
#footBottom {
  width: 100%;
  height: 98rpx;
}
.shopping {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 96rpx;
  background: #FFFFFF;
  border-top: 1px solid #EEEEEE;
  padding: 0 34rpx;
  box-sizing: border-box;
  /* &.noH5:not(.customTabBar){
  	bottom: 0;
  } */
}
.shopping.customTabBar {
  bottom: calc(env(safe-area-inset-bottom) + 98rpx);
}
.shopping.H5 {
  bottom: calc(env(safe-area-inset-bottom) + 98rpx);
}
.shopping .shopping-l .shopping-circle {
  width: 70rpx;
  height: 69rpx;
  line-height: 69rpx;
  background: #fff;
  border-radius: 50%;
  box-shadow: 0 -2rpx 8rpx rgba(0, 0, 0, 0.4);
  position: relative;
}
.shopping .shopping-l .shopping-icon {
  color: #E22319;
  font-size: 45rpx !important;
  text-align: center;
  margin-top: 3rpx;
}
.shopping .shopping-l .shopping-num {
  width: 28rpx;
  height: 28rpx;
  line-height: 28rpx;
  text-align: center;
  background: #E22319;
  border-radius: 50%;
  position: absolute;
  right: 0;
  top: 0;
  font-size: 23rpx;
  color: #fff;
}
.shopping .shopping-l .shopping-price {
  color: #E22319;
  font-size: 36rpx;
  margin-left: 42rpx;
  font-weight: bold;
}
.shopping .shopping-l .shopping-price .shopping-symbol {
  font-size: 22rpx;
}
.shopping .shopping-r {
  width: 178rpx;
  height: 62rpx;
  line-height: 62rpx;
  background: #E22319;
  border-radius: 31rpx;
  color: #FFFFFF;
  font-size: 30rpx;
}